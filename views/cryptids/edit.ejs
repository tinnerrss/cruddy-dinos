<h2>Here's a form to edit <%= cryp.name %></h2>

<form method="POST" action="/cryptids/<%= cryp.id %>/?_method=PUT">
    <label for="cryp-name">Name:</label>
    <input type="text" name="name" id="cryp-name" value="<%= cryp.name %>">
    <label for="cryp-image">Image URL:</label>
    <input type="text" name="img_url" id="cryp-image" value="<%= cryp.img_url %>">
    <input type="submit" value="Update!">
    </form>